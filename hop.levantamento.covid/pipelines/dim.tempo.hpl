    <sql>DECLARE @startNumber INT = ?;
DECLARE @endNumber INT = ?;

WITH gerador (ano) AS (
    SELECT @startNumber AS ano
    UNION ALL
    SELECT ano + 1
    FROM gerador
    WHERE ano + 1 &lt;= @endNumber
)
SELECT ano
FROM gerador
OPTION (MAXRECURSION 0);</sql>
