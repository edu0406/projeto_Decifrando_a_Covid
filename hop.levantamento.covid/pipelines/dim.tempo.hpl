    <lookup>limpa campos</lookup>
    <sql>--Lista de dias
DECLARE @data_ini DATE;
DECLARE @data_fim DATE;


SELECT @data_ini = DATEFROMPARTS(?, 1, 1);
SELECT @data_fim = DATEFROMPARTS(?, 12, 31);

WITH gerador AS (
    SELECT TOP (DATEDIFF(DAY, @data_ini, @data_fim) + 1)
        DATEADD(DAY, ROW_NUMBER() OVER(ORDER BY (SELECT NULL)) - 1, @data_ini) AS data_inicial
    FROM sys.columns c1, sys.columns c2
)
SELECT data_inicial
FROM gerador;</sql>
